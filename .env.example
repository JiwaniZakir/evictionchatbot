# Copy this file to .env and fill in your values.
# Never commit .env to version control.

# OpenAI API key — used server-side by api/chat.js (Vercel serverless function).
# Do NOT prefix with VITE_ — this must never be exposed to the browser bundle.
OPENAI_API_KEY=your_openai_api_key_here

# OpenAI model to use — server-side override, defaults to gpt-4o if not set.
OPENAI_MODEL=gpt-4o
